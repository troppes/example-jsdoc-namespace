{
	"plugins": [],
	"recurseDepth": 10,
	"opts": {
	  "recurse": true,
	  "destination": "./docs/"
	},
	"source": {
	  "include": ["jutge"],
	  "includePattern": ".+\\.js(doc|x)?$",
	  "excludePattern": "(node_modules|tests)"
	},
	"sourceType": "module",
	"tags": {
	  "allowUnknownTags": true,
	  "dictionaries": ["jsdoc", "closure"]
	},
	"templates": {
	  "cleverLinks": false,
	  "monospaceLinks": false,
	  "sort": false
	}
  }